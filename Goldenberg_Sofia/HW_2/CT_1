const num = 12;

if (num % 2 === 0 && num > 10) {
    console.log("The number is even and greater than 10");
}
else if (num % 2 === 0 && num >= 0 && num <=10){
    console.log("The number is even but not greater than 10");
}
else {
    console.log("The number is odd");
}

for (let i = 1; i <= num; i++) {
    console.log(i);
}
